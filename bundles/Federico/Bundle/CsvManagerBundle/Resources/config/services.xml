<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="true" autoconfigure="true"/>

        <service id="Federico\Bundle\CsvManagerBundle\Manager\CsvEncoderFactory\CsvEncoderFactory" />

        <service id="Federico\Bundle\CsvManagerBundle\Manager\CsvManager" />

        <service id="Federico\Bundle\CsvManagerBundle\Commands\PrintCsvCommand" >
            <tag name="console.command" command="app:print-csv"/>
        </service>

    </services>
</container>